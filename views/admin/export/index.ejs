<!DOCTYPE html>
<html lang="en">
<%- include('../partials/head.ejs') %>
<body class="hold-transition sidebar-mini layout-fixed">
<div class="wrapper">
<%- include('../partials/navbar.ejs') %>
<%- include('../partials/sidebar.ejs') %>
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <div class="content-header">
      <div class="container-fluid">
        <div class="row mb-2">
          <div class="col-sm-6">
            <h1 class="m-0">Export Data</h1>
          </div><!-- /.col -->
          <div class="col-sm-6">
            <ol class="breadcrumb float-sm-right">
              <li class="breadcrumb-item"><a href="#">Home</a></li>
              <li class="breadcrumb-item active">Export Sertifikasi</li>
            </ol>
          </div><!-- /.col -->
        </div><!-- /.row -->
      </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->

    <!-- Main content -->
    <section class="content">
      <div class="container-fluid">
        <!-- <div class="row">
          <div class="col-12">
            <div class="card">
              <div class="card-header">
                <h3 class="card-title">Filter</h3>
              </div>
              
              <div class="card-body">
              </div>
            </div>
          </div>
        </div> -->
        <div class="row">
          <div class="col-12">
            <div class="card">
              <div class="card-header">
                <h3 class="card-title">Export Data</h3>
              </div>
              <!-- /.card-header -->
              <div class="card-body">
                <table id="example1" class="table table-bordered table-striped">
                  <thead>
                  <tr>
                    <th>No.Daftar</th>
                    <th>Nama Lengkap</th>
                    <th>NIM</th>
                    <th>NIK</th>
                    <th>Prodi</th>
                    <th>Skema Sertifikasi</th>
                    <th>Tanggal Ujian</th>
                    <th>Hasil Ujian</th>
                    <th>Tujuan Sertifikasi</th>
                    <th>Tanggal Lahir</th>
                    <th>Jenik Kelamin</th>
                    <th>Kebangsaan</th>
                    <th>Alamat</th>
                    <th>Kode Pos</th>
                    <th>No.Telp.HP</th>
                    <th>Email</th>
                    <th>Pekerjaan</th>
                    <th>Jabatan</th>
                    <th>Nama Perusahaan</th>
                    <th>Alamat Perusahaan</th>
                    <th>No.Telp Perusahaan</th>
                    <th>Email Perusahaan</th>
                  </tr>
                  </thead>
                  <tbody>
                    <% data.forEach(element => { %>
                      <tr>
                        <td><%= element.regisNum %></td>
                        <td><%= element.name.toUpperCase() %></td>
                        <td><%= element.nim %></td>
                        <td><%= element.nik %></td>
                        <td><%= element.major.toUpperCase() %></td>
                        <td>
                          <% if (element.schema === 'lp') { %>
                            LEAD PROGRAMMING
                          <% } else { %>
                            NETWORK DESIGN
                          <% } %>
                        </td>
                        <td><%= dateDisplay(element.assesment_date) %></td>
                        <td>
                          <% if (element.assesment_result === 'K') { %>
                            KOMPETEN
                          <% } else if (element.assesment_result === 'BK') { %>
                            BELUM KOMPETEN
                          <% } else { %>
                            BELUM DIKONFORMASI
                          <% } %>
                        </td>
                        <td>
                          <%= element.assesment_type.toUpperCase() %>
                        </td>
                        <td><%= element.birthplace.toUpperCase() %>, <%= dateDisplay(element.date_of_birth) %></td>
                        <td>
                          <%= element.gender === 'L' ? 'LAKI-LAKI' : 'PEREMPUAN' %>
                        </td>
                        <td><%= element.nationality.toUpperCase() %></td>
                        <td><%= element.address.toUpperCase() %></td>
                        <td><%= element.zip_code %></td>
                        <td><%= `+62${element.phone_num}` %></td>
                        <td><%= element.email %></td>
                        <td><%= element.current_job %></td>
                        <td><%= element.position %></td>
                        <td><%= element.company_name %></td>
                        <td><%= element.company_address %></td>
                        <td><%= element.company_telp %></td>
                        <td><%= element.company_email %></td>
                      </tr>
                    <% }) %>
                  </tbody>
                  
                </table>
              </div>
              <!-- /.card-body -->
            </div>
          </div>

        </div>
      </div><!-- /.container-fluid -->
    </section>
    <!-- /.content -->
  </div>
  
<%- include('../partials/footer.ejs') %>
</div>
<!-- ./wrapper -->
<%- include('../partials/js.ejs') %>
</body>
</html>
